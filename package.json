{
  "name": "stylable-playground",
  "version": "1.0.0",
  "description": "A code playground for testing out Stylable",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --open",
    "serve": "serve ./dist",
    "test": "npm run typecheck && npm run lint",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run lint && npm run typecheck && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@file-services/commonjs": "^5.3.1",
    "@file-services/memory": "^5.3.1",
    "@monaco-editor/react": "^4.1.3",
    "@stylable/core": "^4.2.4",
    "@stylable/runtime": "^4.2.4",
    "monaco-editor": "^0.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@file-services/path": "^5.3.1",
    "@file-services/types": "^5.3.1",
    "@stylable/webpack-plugin": "^4.2.4",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-stylable": "^4.2.4",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^9.2.1",
    "typescript": "~4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
