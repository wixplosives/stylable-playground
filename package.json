{
  "name": "stylable-playground",
  "version": "1.0.0",
  "description": "A code playground for testing out Stylable",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --open",
    "serve": "serve ./dist",
    "test": "npm run typecheck && npm run lint",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run lint && npm run typecheck && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@file-services/commonjs": "^5.4.2",
    "@file-services/memory": "^5.4.1",
    "@monaco-editor/react": "^4.2.2",
    "@stylable/core": "^4.6.0",
    "@stylable/runtime": "^4.6.0",
    "monaco-editor": "^0.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@file-services/path": "^5.4.1",
    "@file-services/types": "^5.4.0",
    "@stylable/webpack-plugin": "^4.6.0",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-stylable": "^4.6.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "serve": "^12.0.1",
    "ts-loader": "^9.2.5",
    "typescript": "~4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0"
  }
}
