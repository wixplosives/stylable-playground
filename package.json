{
  "name": "stylable-playground",
  "version": "1.0.0",
  "description": "A code playground for testing out Stylable",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --open",
    "serve": "serve ./dist",
    "pretest": "npm run typecheck && npm run lint",
    "test": "npm run build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run lint && npm run typecheck && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@file-services/commonjs": "^5.7.1",
    "@file-services/memory": "^5.7.1",
    "@monaco-editor/react": "^4.3.1",
    "@stylable/core": "^4.9.5",
    "@stylable/runtime": "^4.9.5",
    "monaco-editor": "^0.31.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@file-services/path": "^5.7.1",
    "@file-services/types": "^5.7.1",
    "@stylable/webpack-plugin": "^4.9.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-stylable": "^4.9.5",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "~4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1"
  }
}
